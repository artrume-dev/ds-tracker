{
  "name": "design-tokens-usage-tracker",
  "version": "1.0.0",
  "description": "A comprehensive dashboard for tracking design tokens usage across teams and digital channels",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm start",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:seed": "cd backend && npm run db:seed",
    "scanner:run": "cd backend && npm run scanner:run"
  },
  "keywords": [
    "design-tokens",
    "design-system",
    "usage-tracker",
    "dashboard",
    "canon"
  ],
  "author": "Canon Design System Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "frontend",
    "backend",
    "shared"
  ],
  "dependencies": {
    "axios": "^1.11.0"
  }
}
